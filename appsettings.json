{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "emeraldwalk.runonsave": {
    "commands": [
      {
        "match": "/extension-project-folder/.*",
        "cmd": "open -a 'Google Chrome' http://reload.extensions"
      }
    ]
  },
  "AllowedHosts": "*"
}
