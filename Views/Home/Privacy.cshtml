@model List<BinghamRailroad.Models.RouteInfoViewModel>

@{
    ViewData["Title"] = "Privacy Policy";
}
<h1>@ViewData["Title"]</h1>

<p>Use this page to detail your site's privacy policy.</p>

@{
    foreach(var m in @Model)
    {
        Console.WriteLine("ID: " + m.RouteId);
        Console.WriteLine("Depart: " + m.DepartureTime);
        Console.WriteLine("Arrive: " + m.ArrivalTime);
        Console.WriteLine("Open Seats: " + m.OpenSeats);
        foreach(var a in m.Amenities)
        {
            Console.WriteLine(a);
        }
    }
}
